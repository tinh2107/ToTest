{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": "off",
    "axe/aria": "off",
    "axe/text-alternatives": "off",
    "disown-opener": "off",
    "no-inline-styles": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]"
        ]
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent"
        ]
      }
    ]
  }
}